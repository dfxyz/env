{
  "workbench.colorTheme": "Candy Paper",
  "workbench.editor.tabActionCloseVisibility": false,
  "workbench.tree.indent": 20,
  "window.commandCenter": false,
  "editor.fontFamily": "'Fira Code', 'Noto Sans CJK SC', 'Noto Sans SC', monospace",
  "editor.fontWeight": "450",
  "editor.lineHeight": 1.6,
  "editor.fontLigatures": true,
  "editor.renderWhitespace": "all",
  "editor.rulers": [80, 120],
  "editor.bracketPairColorization.enabled": false,
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "files.autoSave": "onWindowChange",
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "files.eol": "\n",
  "files.enableTrash": false,
  "terminal.integrated.fontWeight": "450",
  "terminal.integrated.lineHeight": 1.2,
  "terminal.integrated.fontLigatures.enabled": true,
  "terminal.integrated.drawBoldTextInBrightColors": false,
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.profiles.windows": { "zsh": { "path": "zsh.exe" } },
  "terminal.integrated.defaultProfile.windows": "zsh",
  "terminal.integrated.automationProfile.windows": { "path": "cmd.exe" },
  "terminal.external.linuxExec": "alacritty",
  "terminal.external.windowsExec": "alacritty.exe",
  "vscode-neovim.ctrlKeysForNormalMode": [
    "c",
    "d",
    "o",
    "q",
    "r",
    "t",
    "u",
    "v",
    "w",
    "]"
  ],
  "vscode-neovim.ctrlKeysForInsertMode": [
    "c",
    "d",
    "h",
    "j",
    "m",
    "n",
    "o",
    "p",
    "q",
    "r",
    "t",
    "u",
    "w"
  ],
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  }
}
